{
    "name": "OnCore Client",
    "namespace": "OnCoreClient\\ExternalModule",
    "description": "Provides integration with OnCore SIP, SOAP APIs, and UF's OCR API to get protocol and enrollment data into REDCap. <strong><a href=\"https://github.com/ctsit/redcap_oncore_client\">See full documentation here</a></strong>.",
    "permissions": [
        "redcap_every_page_top",
        "redcap_module_system_enable",
        "redcap_module_system_disable"
    ],
    "framework-version": 3,
    "authors": [
        {
            "name": "Tiago Bember",
            "email": "tbembersimeao@ufl.edu",
            "institution": "University of Florida - CTSI"
        },
        {
            "name": "Taryn Stoffs",
            "email": "tls@ufl.edu",
            "institution": "University of Florida - CTSI"
        },
        {
            "name": "Philip Chase",
            "email": "pbc@ufl.edu",
            "institution": "University of Florida - CTSI"
        },
        {
            "name": "Kyle Chesney",
            "email": "kyle.chesney@ufl.edu",
            "institution": "University of Florida - CTSI"
        }
    ],
    "compatibility": {
        "redcap-version-min": "9.0.1"
    },
    "system-settings": [
        {
            "key": "wsdl",
            "name": "WSDL",
            "required": true,
            "type": "text"
        },
        {
            "key": "login",
            "name": "Login",
            "required": true,
            "type": "text"
        },
        {
            "key": "password",
            "name": "Password",
            "required": true,
            "type": "custom",
            "source": "js/password.js",
            "functionName": "OnCoreClient.passwordFieldHandler"
        },
        {
            "key": "protocol_lookup_method",
            "name": "Protocol lookup method",
            "type": "dropdown",
            "required": true,
            "choices": [
                {
                    "name": "SIP",
                    "value": "sip"
                },
                {
                    "name": "API",
                    "value": "api"
                }
            ]
        },
        {
            "key": "sip",
            "name": "SIP URL",
            "type": "text",
                    "branchingLogic": {
                        "field": "protocol_lookup_method",
                        "op": "=",
                        "value": "sip"
                    }
        },
        {
            "key": "ocr_api_url",
            "name": "OCR API URL",
            "type": "text",
                    "branchingLogic": {
                        "field": "protocol_lookup_method",
                        "op": "=",
                        "value": "api"
                    }
        },
        {
            "key": "ocr_api_user",
            "name": "OCR API Username",
            "type": "text",
                    "branchingLogic": {
                        "field": "protocol_lookup_method",
                        "op": "=",
                        "value": "api"
                    }
        },
        {
            "key": "ocr_api_key",
            "name": "OCR API Key",
            "type": "text",
                    "branchingLogic": {
                        "field": "protocol_lookup_method",
                        "op": "=",
                        "value": "api"
                    },
            "source": "js/password.js",
            "functionName": "OnCoreClient.passwordFieldHandler"

        },
        {
            "key": "log_enabled",
            "name": "Log requests",
            "type": "checkbox"
        },
        {
            "key": "sa_send_interval",
            "name": "Frequency to send Summary Accrual data (Hours)",
            "type": "text"
        },
        {
            "key": "use_custom_database",
            "name": "Store Institution IDs in REDCap or Entity database?",
            "type": "radio",
            "choices": [
                {
                    "value": "0",
                    "name": "REDCap Database"
                },
                {
                    "value": "1",
                    "name": "Entity Database"
                }
            ]
        },
        {
            "key": "autopopulate_staff_id",
            "name": "Auto-populate Institution ID from server?</br>(warning, this will overwrite the value in your database)",
            "type": "checkbox"
        },
        {
            "key": "staff_id_server_variable_name",
            "name": "Name of server variable used to populate Institution ID",
            "type": "text",
            "branchingLogic": {
                "field": "autopopulate_staff_id",
                "value": true
            }
        }
    ],
    "project-settings": [
        {
            "key": "protocol_no",
            "name": "Protocol",
            "type": "dropdown",
            "select2": true,
            "required": true,
            "choices": [
                {
                    "value": "",
                    "name": "-- Select --"
                }
            ]
        },
        {
            "key": "using_sa_template",
            "name": "Using a Summary Accrual Template?",
            "type": "checkbox"
        },
        {
            "key": "valid_statuses",
            "name": "Pull subjects having the following statuses",
            "type": "sub_settings",
            "sub_settings": [
                {
                    "key": "off_study",
                    "name": "Off study",
                    "type": "checkbox"
                },
                {
                    "key": "on_study",
                    "name": "On study",
                    "type": "checkbox"
                },
                {
                    "key": "on_treatment",
                    "name": "On treatment",
                    "type": "checkbox"
                },
                {
                    "key": "consented",
                    "name": "Consented",
                    "type": "checkbox"
                },
                {
                    "key": "on_follow_up",
                    "name": "On follow up",
                    "type": "checkbox"
                },
                {
                    "key": "not_eligible",
                    "name": "Not eligible",
                    "type": "checkbox"
                },
                {
                    "key": "_expired",
                    "name": "Expired",
                    "type": "checkbox"
                }
            ]
        },
        {
            "key": "mappings_event",
            "name": "Event to map on",
            "type": "event-list",
            "required": true
        },
        {
            "key": "mappings",
            "name": "Subject Field Mappings (OnCore -> REDCap)",
            "type": "sub_settings",
            "sub_settings": [
                {
                    "key": "primary_identifier",
                    "name": "PrimaryIdentifier",
                    "type": "field-list",
                    "required": true
                },
                {
                    "key": "first_name",
                    "name": "FirstName",
                    "type": "field-list"
                },
                {
                    "key": "middle_name",
                    "name": "MiddleName",
                    "type": "field-list"
                },
                {
                    "key": "last_name",
                    "name": "LastName",
                    "type": "field-list"
                },
                {
                    "key": "suffix",
                    "name": "Suffix",
                    "type": "field-list"
                },
                {
                    "key": "s_s_n",
                    "name": "SSN",
                    "type": "field-list"
                },
                {
                    "key": "gender",
                    "name": "Gender",
                    "type": "field-list"
                },
                {
                    "key": "birth_date",
                    "name": "BirthDate",
                    "type": "field-list"
                },
                {
                    "key": "ethnicity",
                    "name": "Ethnicity",
                    "type": "field-list"
                },
                {
                    "key": "race",
                    "name": "Race",
                    "type": "field-list"
                },
                {
                    "key": "comments",
                    "name": "Comments",
                    "type": "field-list"
                },
                {
                    "key": "contact_info",
                    "name": "ContactInfo",
                    "type": "sub_settings",
                    "sub_settings": [
                        {
                            "key": "address_line",
                            "name": "AddressLine",
                            "repeatable": true,
                            "type": "field-list"
                        },
                        {
                            "key": "city",
                            "name": "City",
                            "type": "field-list"
                        },
                        {
                            "key": "state",
                            "name": "State",
                            "type": "field-list"
                        },
                        {
                            "key": "zip",
                            "name": "Zip",
                            "type": "field-list"
                        },
                        {
                            "key": "county",
                            "name": "County",
                            "type": "field-list"
                        },
                        {
                            "key": "phone_number",
                            "name": "PhoneNumber",
                            "type": "field-list"
                        },
                        {
                            "key": "alternate_phone_number",
                            "name": "AlternatePhoneNumber",
                            "type": "field-list"
                        },
                        {
                            "key": "email_address",
                            "name": "Email",
                            "type": "field-list"
                        }
                    ]
                },
                {
                    "key": "sequence_number",
                    "name": "SequenceNumber",
                    "type": "field-list"
                }
            ]
        }
    ],
    "links": {
        "control-center": [
            {
                "name": "OnCore API Logs",
                "icon": "fas fa-receipt",
                "url": "plugins/api_logs.php"
            },
            {
                "name": "Enter User Institution IDs",
                "icon": "fas fa-users-cog",
                "url": "plugins/oncore_staff_identifier_entry.php"
            }
        ],
        "project": [
            {
                "name": "Pull OnCore Subjects",
                "icon": "fas fa-cloud-download-alt",
                "url": "plugins/subjects_pull.php"
            },
            {
                "name": "Upload Summary Accrual",
                "icon": "fas fa-cloud-upload-alt",
                "url": "plugins/summary_accrual_upload.php"
            },
            {
                "name": "Configure Summary Accrual",
                "icon": "fas fa-cogs",
                "url": "plugins/summary_accrual_configure.php"
            }
        ]
    },
    "crons": [
        {
            "cron_name": "sa_send_cron",
            "cron_description": "Scheduled Summary Accrual Sending",
            "method": "sa_send_cron",
            "cron_frequency": "300",
            "cron_max_run_time": "120"
        }
    ]
}
